class Array {
  make length;
  make arr;

  make Array() {
    length = 1;
    arr = [5];
  }

  make get(x) {;
    send this.arr[x];
  }

  make push(x) {
    arr[length] = x;
    length = length + 1;
  }

  make indexOf(x) {
    loop (make i; i < length; i = i + 1) {
      make test = arr[i];
      
      if (test == x) {
        send i;
      }
    }
  }
};

make x = Array();
x.push("H");
x.push("e");
log(x.indexOf("H"));